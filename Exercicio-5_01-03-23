//5) CRIE UM PROGRAMA QUE LEIA 6 VALORES INTEIROS EM SEGUIDA, MOSTRE OS VALORES EM ORDEM INVERSA

*******************************************************************************/
#include <stdio.h>
#define TAM 6
int main()
{
    int v[TAM];
    
    // entrada
    for (int i=0; i< TAM; i++) {
       printf("Digite um numero v[%d]: ", i);
       scanf ("%d", &v[i]);
    }
    // saida
    printf ("\nEM ORDEM INVERSA:");
    for (int i=TAM-1; i>=0; i--) {
       printf("\nv[%d]=%d", i, v[i]);
    }

    return 0;
}
